<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets
    xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>Codex Sample Document</Title>
      <Author>Peter Martin</Author>
      <Description>Create a sample Codex document.</Description>
      <HelpUrl>www.codexdocs.com/guides</HelpUrl>
      <Shortcut>qdoc</Shortcut>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>doc_title</ID>
          <ToolTip>Replace with the title of the document.</ToolTip>
          <Default>A Sample Document</Default>
        </Literal>
        <Literal>
          <ID>parent_title</ID>
          <ToolTip>Replace with the title of the first section.</ToolTip>
          <Default>First Section</Default>
        </Literal>
        <Literal>
          <ID>child_title</ID>
          <ToolTip>Replace with the title of the first subsection.</ToolTip>
          <Default>First Subsection</Default>
        </Literal>
        <Literal>
          <ID>par1</ID>
          <ToolTip>Replace with the contents of the first paragraph.</ToolTip>
          <Default>
            Insert the content of the first paragraph here
          </Default>
        </Literal>
        <Literal>
          <ID>par2</ID>
          <ToolTip>Replace with the contents of the second paragraph.</ToolTip>
          <Default>
            Insert the content of the second paragraph here
          </Default>
        </Literal>
        <Literal>
          <ID>author</ID>
          <ToolTip>Your name.</ToolTip>
          <Default>
            Name
          </Default>
        </Literal>
        <Literal>
          <ID>desc</ID>
          <ToolTip>Description of the document.</ToolTip>
          <Default>
            This is a Codex document
          </Default>
        </Literal>
      </Declarations>
      <Code Language="XML">
        <![CDATA[<!--
        Author: $author$
        Description: $desc$-->
        
        <document xmlns="http://resources.codexdocs.com/schema/v1/QDocument.xsd" name="myDocument">
          <title>$doc_title$</title>

          <section name="parent_section">
            <title>$parent_title$</title>
              <p>$par1$</p>
            <section name="child_section">
              <title>$child_title$</title>
                <p>$par2$</p>
            </section>
          </section>
        </document>
        ]]>
      </Code>
    </Snippet>
  </CodeSnippet>
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>Codex Button</Title>
      <Author>Peter Martin</Author>
      <Description>Add a button to a Codex document.</Description>
      <HelpUrl>www.codexdocs.com/guides</HelpUrl>
      <Shortcut>qbutton</Shortcut>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>butt_text</ID>
          <ToolTip>Replace with the content string for your button.</ToolTip>
          <Default>Click Me</Default>
        </Literal>
        <Literal>
          <ID>butt_act</ID>
          <ToolTip>Replace with the action for your button.</ToolTip>
          <Default>Enter the script to run when the button is clicked</Default>
        </Literal>
      </Declarations>
      <Code Language="XML">
        <![CDATA[
        <button name="myButton" content="$butt_text$">
          <onClick>
            $butt_act$
          </onClick>
        </button>
        ]]>
      </Code>
    </Snippet>
  </CodeSnippet>
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>Codex Toggle Button</Title>
      <Author>Peter Martin</Author>
      <Description>Add a toggle button to a Codex document.</Description>
      <HelpUrl>www.codexdocs.com/guides</HelpUrl>
      <Shortcut>qtoggle</Shortcut>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>butt_text</ID>
          <ToolTip>Replace with the content string for your button.</ToolTip>
          <Default>Click Me</Default>
        </Literal>
        <Literal>
          <ID>cond1</ID>
          <ToolTip>Replace with the first condition for the button.</ToolTip>
          <Default>Insert the first condition here</Default>
        </Literal>
        <Literal>
          <ID>cond2</ID>
          <ToolTip>Replace with the second condition for the button.</ToolTip>
          <Default>Insert the second condition here</Default>
        </Literal>
      </Declarations>
      <Code Language="XML">
        <![CDATA[
        <script>
          local switch = 0;
        </script>

        <button name="myToggle" content="$butt_text$">
          <onClick>
            switch = 1 - switch;
            if (switch == 0) then
              $cond1$
            else
              $cond2$
            end
          </onClick>
        </button>
        ]]>
      </Code>
    </Snippet>
  </CodeSnippet>
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>Codex Slider</Title>
      <Author>Peter Martin</Author>
      <Description>Add a slider to a Codex document.</Description>
      <HelpUrl>www.codexdocs.com/guides</HelpUrl>
      <Shortcut>qslider</Shortcut>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>slide_min</ID>
          <ToolTip>Replace with minimum slider value.</ToolTip>
          <Default>0</Default>
        </Literal>
        <Literal>
          <ID>slide_max</ID>
          <ToolTip>Replace with maximum slider value.</ToolTip>
          <Default>10</Default>
        </Literal>
        <Literal>
          <ID>slide_val</ID>
          <ToolTip>Replace with current slider value.</ToolTip>
          <Default>5</Default>
        </Literal>
        <Literal>
          <ID>slide_act</ID>
          <ToolTip>Replace with the slider action.</ToolTip>
          <Default>Enter your slide action here</Default>
        </Literal>
      </Declarations>
      <Code Language="XML">
        <![CDATA[
        <slider name="mySlider" min="$slide_min$" max="$slide_max$" value="$slide_val$">
          <onValueChanged>
            $slide_act$
          </onValueChanged>
        </slider>
        ]]>
      </Code>
    </Snippet>
  </CodeSnippet>
