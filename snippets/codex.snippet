    <CodeSnippet Format="1.0.0">
    <Header>
      <Title>Codex Simulation Start/Stop Button</Title>
      <Author>Peter Martin</Author>
      <Description>Add a button that controls a simulation to a Codex document.</Description>
      <HelpUrl>www.codexdocs.com/guides</HelpUrl>
      <Shortcut>qsimbutton</Shortcut>
    </Header>
    <Snippet>
      <Code Language="XML">
        <![CDATA[
        <button name="sim_button" content="Start">
          <onClick>
            if sim_button.Text == "Start" then
              sim_button.Text = "Stop";
              sim:Start();
            else
              sim_button.Text = "Start";
              sim:Stop();
            end
          </onClick>
        </button>
        ]]>
      </Code>
    </Snippet>
  </CodeSnippet>
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>Codex Simulation Script</Title>
      <Author>Peter Martin</Author>
      <Description>Add a simulation that runs a script.</Description>
      <HelpUrl>www.codexdocs.com/guides</HelpUrl>
      <Shortcut>qsimscript</Shortcut>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>duration</ID>
          <ToolTip>Replace with the duration of the simulation.</ToolTip>
          <Default>10</Default>
        </Literal>
        <Literal>
          <ID>period</ID>
          <ToolTip>Replace with the period of the simulation.</ToolTip>
          <Default>0.1</Default>
        </Literal>
        <Literal>
          <ID>onupdate</ID>
          <ToolTip>Replace with the script to run at each time step.</ToolTip>
          <Default><![CDATA[<!-- Insert your script -->]]></Default>
        </Literal>
        <Literal>
          <ID>onduration</ID>
          <ToolTip>Replace with the script to run when the simulation is complete.</ToolTip>
          <Default><![CDATA[<!-- sim_button.Text = "Start"; -->]]></Default>
        </Literal>
      </Declarations>
      <Code Language="XML">
        <![CDATA[
        <simulation duration="$duration$" name="sim" period="$period$">
          <onUpdate>
            $onupdate$
          </onUpdate>
          <onDuration>
            $onduration$
          </onDuration>
        </simulation>
        ]]>
      </Code>
    </Snippet>
  </CodeSnippet>
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>Codex Simulation Solver</Title>
      <Author>Peter Martin</Author>
      <Description>Add a simulation that includes a solver.</Description>
      <HelpUrl>www.codexdocs.com/guides</HelpUrl>
      <Shortcut>qsimsolve</Shortcut>
