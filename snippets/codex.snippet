  </CodeSnippet>
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>Codex PID Controller</Title>
      <Author>Peter Martin</Author>
      <Description>Add a PID controller into a simulation solver.</Description>
      <HelpUrl>www.codexdocs.com/guides</HelpUrl>
      <Shortcut>qpid</Shortcut>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>amp</ID>
          <ToolTip>Replace with the amplitude of the command.</ToolTip>
          <Default>1</Default>
        </Literal>
        <Literal>
          <ID>freq</ID>
          <ToolTip>Replace with the frequency of the command.</ToolTip>
          <Default>0.1</Default>
        </Literal>
        <Literal>
          <ID>P</ID>
          <ToolTip>Replace with the proportional gain.</ToolTip>
          <Default>1</Default>
        </Literal>
        <Literal>
          <ID>I</ID>
          <ToolTip>Replace with the integral gain.</ToolTip>
          <Default>1</Default>
        </Literal>
        <Literal>
          <ID>D</ID>
          <ToolTip>Replace with the derivative gain.</ToolTip>
          <Default>1</Default>
        </Literal>
        <Literal>
          <ID>plant</ID>
          <ToolTip>Replace with the plant model.</ToolTip>
          <Default><![CDATA[<transferFunction name="plant" num="10" den="0.1 1" />]]></Default>
        </Literal>
      </Declarations>
      <Code Language="XML">
        <![CDATA[
        <series>
          <!-- Command -->
          <square name="mysquare" amplitude="$amo$" frequency="$freq$" />

          <!-- Feedback loop -->
          <feedback>
            <!-- Forward path -->
            <series>
              <!-- Controller -->
              <parallel>
                <!-- Proportional -->
                <gain name="Kp" value="$P$" />

                <!--- Integral -->
                <series>
                  <gain name="Ki" value="$I$" />
                  <transferFunction name="integrator" num="1" den="1 0" />
                </series>

                <!-- Derivative -->
                <series>
                  <gain name="Kd" value="$D$" />
                  <transferFunction name="derivative" num="1 0" den="1" />
                </series>
              </parallel>

              <!-- Plant -->
              $plant$
            </series>

            <!-- Feedback path -->
            <!-- Unity gain feedback since no system in feedback path -->
          </feedback>
        </series>
        ]]>
      </Code>
    </Snippet>
  </CodeSnippet>
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>Codex Data Plot</Title>
      <Author>Peter Martin</Author>
      <Description>Add a data plot.</Description>
      <HelpUrl>www.codexdocs.com/guides</HelpUrl>
      <Shortcut>qdataplot</Shortcut>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>xmin</ID>
          <ToolTip>Replace with the x-axis minimum.</ToolTip>
          <Default>0</Default>
        </Literal>
        <Literal>
          <ID>xmax</ID>
          <ToolTip>Replace with the x-axis maximum.</ToolTip>
          <Default>10</Default>
        </Literal>
        <Literal>
          <ID>ymin</ID>
          <ToolTip>Replace with the y-axis minimum.</ToolTip>
          <Default>0</Default>
        </Literal>
        <Literal>
          <ID>ymax</ID>
          <ToolTip>Replace with the y-axis maximum.</ToolTip>
          <Default>10</Default>
        </Literal>
        <Literal>
          <ID>data</ID>
          <ToolTip>Replace with a dataset.</ToolTip>
          <Default>0 0; 1 1; 2 2; 3 3</Default>
        </Literal>
      </Declarations>
      <Code Language="XML">
        <![CDATA[
        <drawing>
          <axis name="xaxis" dim="x" min="$xmin$" max="$xmax$"/>
          <axis name="yaxis" dim="y" min="$ymin$" max="$ymax$"/>
          <plot>
            <series>
              <data>$data$</data>
            </series>
          </plot>
        </drawing>
        ]]>
      </Code>
    </Snippet>
  </CodeSnippet>
