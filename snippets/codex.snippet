  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>Codex Table</Title>
      <Author>Peter Martin</Author>
      <Description>Add a table to a Codex document.</Description>
      <HelpUrl>www.codexdocs.com/guides</HelpUrl>
      <Shortcut>qtable</Shortcut>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>col1</ID>
          <ToolTip>Replace with the name of the first column.</ToolTip>
          <Default>Title 1</Default>
        </Literal>
        <Literal>
          <ID>col2</ID>
          <ToolTip>Replace with the name of the second column.</ToolTip>
          <Default>Title 2</Default>
        </Literal>
        <Literal>
          <ID>val11</ID>
          <ToolTip>Replace with the first value in the first row.</ToolTip>
          <Default>(0, 0)</Default>
        </Literal>
        <Literal>
          <ID>val12</ID>
          <ToolTip>Replace with the second value in the first row.</ToolTip>
          <Default>(0, 1)</Default>
        </Literal>
        <Literal>
          <ID>val21</ID>
          <ToolTip>Replace with the first value in the second row.</ToolTip>
          <Default>(1, 0)</Default>
        </Literal>
        <Literal>
          <ID>val22</ID>
          <ToolTip>Replace with the second value in the second row.</ToolTip>
          <Default>(1, 1)</Default>
        </Literal>
      </Declarations>
      <Code Language="XML">
        <![CDATA[
          <table grid="true" widths="50% 50%">
            <head>
              <column>
                <p>$col1$</p>
              </column>
              <column>
                <p>$col2$</p>
              </column>
            </head>
            <row>
              <column>
                <p>$val11$</p>
              </column>
              <column>
                <p>$val12$</p>
              </column>
            </row>
            <row>
              <column>
                <p>$val21$</p>
              </column>
              <column>
                <p>$val22$</p>
              </column>
            </row>
          </table>
        ]]>
      </Code>
    </Snippet>
  </CodeSnippet>
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>Codex List</Title>
      <Author>Peter Martin</Author>
      <Description>Add a list to a Codex document.</Description>
      <HelpUrl>www.codexdocs.com/guides</HelpUrl>
      <Shortcut>qlist</Shortcut>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>type</ID>
          <ToolTip>Replace with the type of list.</ToolTip>
          <Default>decimal</Default>
        </Literal>
        <Literal>
          <ID>item1</ID>
          <ToolTip>Replace with the first item.</ToolTip>
          <Default>First item</Default>
        </Literal>
        <Literal>
          <ID>item2</ID>
          <ToolTip>Replace with the second item.</ToolTip>
          <Default>Second item</Default>
        </Literal>
      </Declarations>
      <Code Language="XML">
        <![CDATA[
        <style>
          <counter name="listcount" format="$type$" />
        </style>

        <style name="mylist">
          <increment counter="listcount" />
          <numbering counter="listcount" mode="flat" />
        </style>

        <p style="mylist">
          $item1$
        </p>
        <p style="mylist">
          $item2$
        </p>
        ]]>
      </Code>
    </Snippet>
  </CodeSnippet>
